{
  "name": "goetas-webservices/wsdl2php",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email": "goetas@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "goetas-webservices/soap-reader": "@dev",
    "goetas-webservices/wsdl-reader": "@dev",
    "goetas-webservices/xsd2php": "@dev",
    "symfony/dependency-injection": "^4.0|^5.0",
    "symfony/config": "^4.0|^5.0",
    "symfony/yaml": "^4.0|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "jms/serializer": "^1.2|^2.0|^3.0",
    "goetas-webservices/xsd2php-runtime": "@dev"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\WsdlToPhp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\WsdlToPhp\\Tests\\": "tests",
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "vendor/goetas-webservices/xsd2php/tests"
    }
  },
  "bin": [
    "bin/wsdl2php"
  ]
}
